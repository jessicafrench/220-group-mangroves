1661460401754:#Create one column of month and year in date format so that nitrogen concentration can be summarized by month and year.
1661460401756:date_column_only <- combined_data |>
1661460401757:select(Sample_Date, Sample_ID)
1661460401764:combined_data_mean <- combined_data |>
1661460401765:mutate(Month_Year = paste(month(Sample_Date), year(Sample_Date))) |>
1661460401766:mutate(Month_Year = my(Month_Year)) |>
1661460401766:group_by(Month_Year, Sample_ID) |>
1661460401767:summarize(Monthly_Mean_N = mean(`NO3-N`, na.rm = TRUE),
1661460401767:Monthly_Mean_K = mean(K, na.rm = TRUE))
