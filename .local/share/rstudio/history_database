1661534731494:# Chunk 4
1661534731495:columns_vector <- c("Sample_ID", "Sample_Date", "NO3-N", "K") #vector of columns to be kept from data frames.
1661534731496:#select columns form columns vector, create columns for month and year and filter to rows that contain years 1988 to 1994.
1661534731496:bq1_tidy <- bq1 |>
1661534731497:select(columns_vector) |> #---- subset to include only the columns (variables) we want
1661534731497:mutate(Year = year(Sample_Date), Month = month(Sample_Date)) |>   #---- add a new column with year - month combination
1661534731498:filter(Year %in% c(seq(from = 1988, to = 1994))) #---- subset only for rows in the time frame of interest
1661534731547:bq2_tidy <- bq2 |>   #---- complete the exact same steps for the following three data sets
1661534731547:select(columns_vector) |>
1661534731547:mutate(Year = year(Sample_Date), Month = month(Sample_Date)) |>
1661534731548:filter(Year %in% c(seq(from = 1988, to = 1994)))
1661534731565:bq3_tidy <- bq3 |>
1661534731565:select(columns_vector) |>
1661534731565:mutate(Year = year(Sample_Date), Month = month(Sample_Date)) |>
1661534731566:filter(Year %in% c(seq(from = 1988, to = 1994)))
1661534731583:prm_tidy <- prm |>
1661534731583:select(columns_vector) |>
1661534731584:mutate(Year = year(Sample_Date), Month = month(Sample_Date)) |>
1661534731584:filter(Year %in% c(seq(from = 1988, to = 1994)))
